Gray aload length {
    aload pop
    /light exch def
    /saturation exch def
    /hue exch def
    /left Left def

    [-2 0 2] {
        saturation add 
        [-2 0 2] {
            hue add exch dup 3 1 roll
            left Top 4 2 roll light DrawFig
            /left left width add def
        } forall
